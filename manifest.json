{
  "manifest_version": 3,
  "name": "Copy Paste File Text",
  "version": "1.0",
  "description": "A Chrome extension to extract text from various file formats",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ]
} 